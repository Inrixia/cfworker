{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "esnext",
    "target": "ES2021",
    "checkJs": true,
    "resolveJsonModule": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true
  }
}
